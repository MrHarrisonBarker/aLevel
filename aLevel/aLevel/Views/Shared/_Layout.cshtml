<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title - MoreTwitterKnowledge</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/d3")

</head>
<body>

    <div class="container body-content" style="min-height: 100%">

        @RenderBody()
        
        
        

        <div class="footer navbar-fixed-bottom">
            
            <a href="https://www.twitter.com"><img src="~/img/twitterlogo.png" class="center-block Twit-logo" style="margin-bottom: 1%;margin-top: 1%" /></a>

            <p style="color: grey; padding: 0 2%"; align="center">&copy; @DateTime.Now.Year - MoreTwitterknowledge</p>

        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/tablesorter")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
